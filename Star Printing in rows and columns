#include<iostream>
using namespace std;

int main(){
   int n;
   cout<<"Enter the value of n:\n";
   cin>> n;

   int m;
   cout<<"Enter the value of m:\n";
   cin>> m;
   for(int i=1; i<=n ; i++){ //Line
        for(int i=1; i<=m ; i++){ // Numer of stars
        cout<<"*"<< " ";
   }
    cout<<endl;
}
    return 0;
}
